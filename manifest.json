{
    "manifest_version": 3,
    "name": "CSV Search Plugin",
    "description": "Search CSV file for entered name",
    "version": "1.0",
    "permissions": ["storage"],
    "action": {
      "default_popup": "popup.html"
    },
    "background": {
      "service_worker": "background.js"
    },
    "icons": {
      "128": "icon128.png"
    },
    "host_permissions": [
      "file://*/*"
    ]
  }
  